 #include<iostream>
 #include<vector>
 using namespace std;
 int main(){
 
 vector <int> p(5);
 
//auto is for auto detection of the data type  --- a belong to p. 
for(auto a: p)
    cout<< a << " ";

 return 0;
 }
 
 
 
