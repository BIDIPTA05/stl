#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int> a{5,3,2,8,9,10};
    vector<int> b;
//pushing elements
    for (int i = 0; i < a.size();i++)
    {
        b.push_back(a[i]);
    }

//displaying elements of the new vector
  for (int i = 0; i < a.size();i++)
    {
        cout << b[i] << endl;
    }


    return 0;
}


*****************************************************************


#include<iostream>
#include<vector>
using namespace std;
int main(){

    vector<int> a{1,3,4,5,6,7,15};
    vector<int> b;
    //this is the = operator
    b = a;
    

    for (int i = 0; i < a.size();i++)
    {
        cout << b[i] << " ";
    }

        return 0;
}



****************************************************************************


#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int> a{5,3,2,8,9,10};
    vector<int> b(a.begin(), a.end());

    for (int i = 0; i < a.size();i++)
    {
        cout << b[i] << " ";
    }
    return 0;
}
